    "db:pull": "npm run db:pull:auth && npm run db:pull:public",
    "db:pull:auth": "drizzle-kit pull --config=drizzle.config.auth.ts",
    "db:pull:public": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
